{
  "name": "another-doctor-monorepo",
  "version": "0.1.0",
  "private": true,
  "description": "Another Doctor - Medical specialist matching service",
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "type-check": "turbo run type-check",
    "clean": "turbo run clean",
    "dev:frontend": "turbo run dev --filter=apps/frontend",
    "dev:workers": "turbo run dev --filter=infra/cloudflare/workers",
    "build:frontend": "turbo run build --filter=apps/frontend",
    "build:workers": "turbo run build --filter=infra/cloudflare/workers",
    "lint:all": "turbo run lint",
    "test:all": "turbo run test"
  },
  "devDependencies": {
    "turbo": "^1.10.16",
    "@changesets/cli": "^2.26.2"
  },
  "packageManager": "pnpm@8.7.0",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "workspaces": [
    "apps/frontend",
    "packages/ts-utils",
    "infra/cloudflare/workers"
  ]
}