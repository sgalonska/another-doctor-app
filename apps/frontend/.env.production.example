# Frontend Production Environment Configuration
# Copy this file to .env.production and update values

# Application
NODE_ENV=production
NEXT_PUBLIC_ENVIRONMENT=production

# API Configuration
NEXT_PUBLIC_API_URL=https://backend-service-url.run.app/api/v1
NEXT_PUBLIC_WORKERS_URL=https://workers-service-url.run.app

# Analytics (if using)
NEXT_PUBLIC_GA_ID=your-google-analytics-id
NEXT_PUBLIC_MIXPANEL_TOKEN=your-mixpanel-token

# Error Monitoring
NEXT_PUBLIC_SENTRY_DSN=your-frontend-sentry-dsn

# Feature Flags
NEXT_PUBLIC_ENABLE_ANALYTICS=true
NEXT_PUBLIC_ENABLE_ERROR_REPORTING=true
NEXT_PUBLIC_MAINTENANCE_MODE=false

# CDN and Assets
NEXT_PUBLIC_CDN_URL=https://storage.googleapis.com/PROJECT_ID-another-doctor-prod-storage